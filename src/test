(def one(x) (+ 1 x))
(def two(x) (+ 2 x))
(def three(x) (+ 3 x))
(def all_3 ()
     (+ (three 2)
     (two (one 2))))
(-> all_3)
(def three(x) 
     (+ (one x) 
        (two x)))
(def all_3 ()
     (+ (three 2)
     (two (one 2))))
(-> all_3)
(query (where 0 '-> 2))
(query 'lists (where 2 'x '-> '(x x)))
